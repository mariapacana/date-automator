<div id="crush_main" class="main">
  <div id="new_crush_div">
    <h2>ADD CRUSHES</h2>

    <form action="/oauth_google" method="get">
      <button id="import">Add Google Contacts</button>
    </form>

    <div id="contacts">
      <% if @contacts %>
        <%= erb :_contact_list %>
      <% end %>
    </div>

    <form id="new_crush_form">
      <%= erb :_crush %>
      <input id="crushes_submit" type="submit" value="Submit">
    </form>
  </div>

  <div id="all_crushes">
    <%= erb :_all_crushes %>
  </div>

</div>
