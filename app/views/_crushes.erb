<div id="crush_main" class="main">
  <div id="new_crush_div">
    <h2>ADD CRUSHES</h2>

    <form method="get" action="/oauth_tofb">
      <input value="Add Facebook Contacts" type="submit" />
    </form>

    <form id="new_crush_form">
      <%= erb :_crush %>
      <input id="crushes_submit" type="submit" value="Submit">
    </form>
  </div>

  <div id="all_crushes">
    <%= erb :_all_crushes %>
  </div>
</div>
