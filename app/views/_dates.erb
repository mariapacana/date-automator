<div class="main">

  <% if !currentuser.google_opt_out && !currentuser.enabled_google_oauth? %>
    <%= erb :_g_oauth %>
  <% end %>

  <div id="new_date_div">
    <h2>ADD TIMES</h2>

    <form id="new_date_form">
      <%= erb :_datetime %>
      <input id="schedule_submit" type="submit" value="Submit">
    </form>
  </div>

  <div id="all_free_dates">
    <%= erb :_all_free_dates %>
  </div>
</div>
